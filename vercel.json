{
  "version": 2,
  "buildCommand": "nx build watch-together",
  "outputDirectory": "dist/apps/watch-together/browser",
  "builds": [
    {
      "src": "apps/backend-api/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/apps/backend-api/src/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/apps/backend-api/src/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
